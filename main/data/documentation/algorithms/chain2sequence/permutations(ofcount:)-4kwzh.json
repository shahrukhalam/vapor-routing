ype":"text","text":" "},{"type":"text","text":"is the number of elements in the base collection, since"},{"type":"text","text":" "},{"type":"codeVoice","code":"PermutationsSequence"},{"type":"text","text":" accesses the "},{"type":"codeVoice","code":"count"},{"type":"text","text":" of the base collection."}]}]}]}],"identifier":{"url":"doc:\/\/co.pointfree.VaporRouting\/documentation\/Algorithms\/Chain2Sequence\/permutations(ofCount:)-4kwzh","interfaceLanguage":"swift"},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/algorithms\/chain2sequence\/permutations(ofcount:)-4kwzh"]}],"abstract":[{"text":"Returns a collection of the permutations of this collection of the","type":"text"},{"text":" ","type":"text"},{"text":"specified length.","type":"text"}],"sections":[],"schemaVersion":{"minor":3,"patch":0,"major":0},"metadata":{"modules":[{"name":"Algorithms"}],"externalID":"s:Sl10AlgorithmsE12permutations7ofCountAA20PermutationsSequenceVyxGSiSg_tF::SYNTHESIZED::s:10Algorithms14Chain2SequenceV","symbolKind":"method","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"permutations"},{"kind":"text","text":"("},{"text":"ofCount","kind":"externalParam"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:Si","text":"Int","kind":"typeIdentifier"},{"text":"?) -> ","kind":"text"},{"preciseIdentifier":"s:10Algorithms20PermutationsSequenceV","text":"PermutationsSequence","kind":"typeIdentifier"},{"text":"<","kind":"text"},{"text":"Self","kind":"typeIdentifier"},{"text":">","kind":"text"}],"extendedModule":"Swift","role":"symbol","title":"permutations(ofCount:)","roleHeading":"Instance Method"},"kind":"symbol","hierarchy":{"paths":[["doc:\/\/co.pointfree.VaporRouting\/documentation\/Algorithms","doc:\/\/co.pointfree.VaporRouting\/documentation\/Algorithms\/Chain2Sequence","doc:\/\/co.pointfree.VaporRouting\/documentation\/Algorithms\/Chain2Sequence\/Collection-Implementations"]]},"references":{"doc://c